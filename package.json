{
    "name": "ascii-name",
    "version": "2.0.0",
    "description": "Return name(s) of ASCII code characters in multiple languages.",
    "type": "module",
    "author": {
        "name": "Heeseong Yoo",
        "email": "heeseong.yoo@gmail.com"
    },
    "license": "MIT",
    "repository": "avocadee/ascii-name",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist",
        "ascii-name_*.json"
    ],
    "scripts": {
        "build": "tsup src/index.js --format cjs,esm --dts --clean",
        "dev": "tsup src/index.js --format cjs,esm --watch",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js index.test.js",
        "prepare": "npm run build"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    },
    "overrides": {
        "minimist": "^1.2.6"
    }
}